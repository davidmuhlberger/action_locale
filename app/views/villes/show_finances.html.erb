                      <% elsif 100 * @comptes_2016[:endettement_16].to_i / (@produits_fonctionnement_2016 + @charges_fonctionnement_2016) > 200 %>
                      <%= image_tag "capa_fin_max.png" %>
                    <% else %>
                      <%= image_tag "capa_fin_too_much.png" %>
                    <% end %>
                  </td>
                </tr>
              </tbody>
            </table>
